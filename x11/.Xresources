! font settings --------------------------------------------------------------
Xft.autohint: false
Xft.antialias: true
Xft.dpi: 96
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! rofi theme -----------------------------------------------------------------
rofi.color-enabled:    true
rofi.color-normal:     argb:00000000,  #D8DEE9 , argb:00000000,  #FAC863 ,  #000000
rofi.color-urgent:     argb:00000000,  #F99157 , argb:00000000,  #F99157 ,  #000000
rofi.color-active:     argb:00000000,  #000000 , argb:00000000,  #000000 ,  #000000
rofi.color-window:     argb:ee222222,  #FAC863 ,  #FAC863
rofi.fuzzy:            false

!!!!!!!!!
! URxvt !
!!!!!!!!!

! Fonts
URxvt.font: xft:Hack:size=11,style=medium
URxvt.boldFont: xft:Hack:size=11,style=bold
URxvt.italicFont: xft:Hack:size=11,style=bold
URxvt.boldItalicFont: xft:Hack:size=11,style=bold
URxvt.letterSpace: -1

! UI
URxvt*borderless: true
!URxvt.internalBorder: 10
URxvt*scrollBar: false
URxvt*saveLines: 1000
!URxvt*dynamicColors: true
URxvt*termName: rxvt-unicode
URxvt*fading: 10

! Colors
URxvt.depth: 32
URxvt.intensityStyles: false

! Extensions
URxvt.perl-ext: default,url-select,clipboard
URxvt.perl-ext-common: default,matcher

! URxvt.keysym.Control-Alt-C: perl:clipboard:copy
! URxvt.keysym.Control-Alt-V: perl:clipboard:paste

URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1

! Tartan theme from xcolors.net
! from http://www.xcolors.net/dl/tartan
!urxvt color scheme:
URxvt*background: #202020
URxvt*foreground: #DEDEDE
URxvt*colorUL: #86a2b0
! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
